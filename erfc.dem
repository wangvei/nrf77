	PROGRAM D6R11
C	Driver for routine ERFC
	CHARACTER TEXT*14
	OPEN(5,FILE='FNCVAL.DAT',STATUS='OLD')
10	READ(5,'(A)') TEXT
	IF (TEXT.NE.'Error Function') GOTO 10
	READ(5,*) NVAL
	WRITE(*,*) 'Complementary error function'
	WRITE(*,'(1X,T5,A1,T12,A6,T23,A7)')
     *		'X','Actual','ERFC(X)'
	DO 11 I=1,NVAL
		READ(5,*) X,VALUE
		VALUE=1.0-VALUE
		WRITE(*,'(F6.2,2F12.7)') X,VALUE,ERFC(X)
11	CONTINUE
	CLOSE(5)
	END
