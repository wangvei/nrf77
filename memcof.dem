	PROGRAM D12R9
C	Driver for routine MEMCOF
	PARAMETER(N=1000,M=10)
	DIMENSION DATA(N),COF(M),WK1(N),WK2(N),WKM(M)
	OPEN(5,FILE='SPCTRL.DAT',STATUS='OLD')
	READ(5,*) (DATA(I),I=1,N)
	CLOSE(5)
	CALL MEMCOF(DATA,N,M,PM,COF,WK1,WK2,WKM)
	WRITE(*,'(/1X,A/)') 'Coeff. for spectral estim. of SPCTRL.DAT'
	DO 11 I=1,M
		WRITE(*,'(1X,A,I2,A,F12.6)') 'a[',I,'] =',COF(I)
11	CONTINUE
	WRITE(*,'(/1X,A,F12.6/)') 'b0 =',PM
	END
