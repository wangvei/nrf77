	PROGRAM D8R9
C	Driver for routine SHELL
	DIMENSION A(100)
	OPEN(5,FILE='TARRAY.DAT',STATUS='OLD')
	READ(5,*) (A(I),I=1,100)
	CLOSE(5)
C	Print original array
	WRITE(*,*) 'Original array:'
	DO 11 I=1,10
		WRITE(*,'(1X,10F6.2)') (A(10*(I-1)+J),J=1,10)
11	CONTINUE
C	Sort array
	CALL SHELL(100,A)
C	Print sorted array
	WRITE(*,*) 'Sorted array:'
	DO 12 I=1,10
		WRITE(*,'(1X,10F6.2)') (A(10*(I-1)+J),J=1,10)
12	CONTINUE
	END
