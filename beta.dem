	PROGRAM D6R5
C	Driver for routine BETA
	CHARACTER TEXT*13
	OPEN(5,FILE='FNCVAL.DAT',STATUS='OLD')
10	READ(5,'(A)') TEXT
	IF (TEXT.NE.'Beta Function') GOTO 10
	READ(5,*) NVAL
	WRITE(*,*) TEXT
	WRITE(*,'(1X,T5,A1,T11,A1,T24,A6,T43,A9)')
     *		'W','Z','Actual','BETA(W,Z)'
	DO 11 I=1,NVAL
		READ(5,*) W,Z,VALUE
		WRITE(*,'(2F6.2,2E20.6)') W,Z,VALUE,BETA(W,Z)
11	CONTINUE
	CLOSE(5)
	END
